<div id="<%= dom_id user %>">
  <p>
    <strong>Unique id:</strong>
    <%= user.unique_id %>
  </p>

  <p>
    <strong>Nickname:</strong>
    <%= user.nickname %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Affiliation:</strong>
    <%= user.affiliation %>
  </p>

   <p>
    <strong>Display Name:</strong>
    <%= user.display_name %>
  </p>

  <p>
    <strong>Box No:</strong>
    <%= user.box_no %>
  </p>

  <p>
    <strong>Is Admin:</strong>
    <%= user.is_admin %>
  </p>

</div>
